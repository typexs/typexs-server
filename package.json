{
  "name": "@typexs/server",
  "version": "0.3.3",
  "description": "Servers for typexs",
  "main": "index.js",
  "directories": {
    "doc": "docs/de"
  },
  "scripts": {
    "test": "nyc mocha --opts test/mocha.all.opts",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typexs/typexs-server.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "commons",
    "typescript",
    "typexs",
    "server",
    "controllers"
  ],
  "bugs": {
    "url": "https://github.com/typexs/typexs-server/issues"
  },
  "homepage": "https://github.com/typexs/typexs-server#readme",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/express": "^4.16.0",
    "@types/gulp": "^4.0.5",
    "@types/gulp-watch": "^4.1.34",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.11.3",
    "@types/request-promise": "^4.1.42",
    "@types/superagent": "^3.8.4",
    "@types/supertest": "^2.0.6",
    "@types/supertest-as-promised": "^2.0.36",
    "@types/winston": "^2.4.4",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "del": "^3.0.0",
    "express": "^4.16.3",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "^0.6.5",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.1",
    "gulpclass": "^0.1.2",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "nyc": "^13.0.1",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "sqlite3": "^4.0.2",
    "supertest": "^3.3.0",
    "supertest-as-promised": "^4.0.2",
    "ts-node": "^6.2.0",
    "typescript": "^3.2.4",
    "typescript-require": "^0.2.10"
  },
  "dependencies": {
    "@typexs/base": "latest",
    "routing-controllers": "^0.7.7"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false
  },
  "typexs": {
    "name": "server",
    "declareLibs": [
      {
        "topic": "server.controllers",
        "refs": [
          "controllers",
          "src/controllers",
          "src/modules/*/controllers"
        ]
      },
      {
        "topic": "server.middleware",
        "refs": [
          "middleware",
          "src/middleware"
        ]
      }
    ]
  }
}
