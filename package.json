{
  "name": "typexs-server",
  "version": "0.0.1",
  "dependencies": {
    "lodash": "^4.17.4",
    "routing-controllers": "^0.7.6",
    "typexs-base": "^0.2.32"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/express": "^4.0.37",
    "@types/lodash": "^4.14.92",
    "@types/node": "^9.3.0",
    "chai": "^4.1.0",
    "codelyzer": "~3.2.0",
    "del": "^3.0.0",
    "express": "^4.16.2",
    "gulp-bump": "^2.8.0",
    "gulp-replace": "^0.6.1",
    "gulp-shell": "^0.6.3",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.2",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "gulpclass": "^0.1.2",
    "mocha": "^2.2.5",
    "mocha-typescript": "^1.1.9",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2",
    "typescript-require": "^0.2.9-1"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false
  },
  "typexs": {
    "name": "server",
    "declareLibs": [
      {
        "topic": "server.controllers",
        "refs": [
          "controllers",
          "src/controllers",
          "src/components/*/controllers"
        ]
      }
    ]
  }
}
